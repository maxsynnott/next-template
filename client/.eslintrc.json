{
	"extends": "next/core-web-vitals",
	"rules": {
		"no-restricted-imports": [
			"error",
			{
				"paths": [
					{
						"name": "@nextui-org/react",
						"message": "Import from individual package instead"
					},
					{
						"name": "next/router",
						"message": "Use next/navigation instead"
					},
					{
						"name": "next/link",
						"message": "Use @nextui-org/link instead"
					}
				]
			}
		]
	}
}
